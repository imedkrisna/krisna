library(tidyverse)
demand <- function(q) 1000-q
chart <- ggplot(data = data.frame(x = 0), mapping = aes(x = x)) +
stat_function(fun = demand,size=1.1, color='blue') +
labs(x='Q',y='P')+
xlim(0,900)+
geom_hline(yintercept=500, color = "red",size=1.1)+
geom_ribbon(aes(x=c(0,500), ymax=demand(0), ymin = 500,
fill = "Joy's surplus"), alpha = 0.25, show.legend = FALSE)+
dark_theme_bw(base_size=25)+
theme(text=element_text(color='gray'))
